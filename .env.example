# Telegram Bot Token (получите у @BotFather)
bot_token=ваш_телеграм_токен_здесь

# Пароль для загрузки файлов
upload_password=ваш_пароль_здесь

# Google Drive настройки

# OAuth2 credentials для загрузки файлов (обычный credentials.json от Google Cloud)
OAUTH_CREDENTIALS_PATH=credentials.json

# Service Account credentials для чтения файлов (опционально)
# Если не указан, будет использоваться OAuth2 для всех операций
SERVICE_CREDENTIALS_PATH=service_credentials.json

# ID папки Google Drive (опционально, если не указан - создастся папка)
# folder_id=1ABC-xyz123_folder_id_here

# Имя папки (создастся автоматически если не существует)
folder_name=MinecraftMods
