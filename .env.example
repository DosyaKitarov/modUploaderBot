# Telegram Bot Token (get from @BotFather)
bot_token=YOUR_TELEGRAM_BOT_TOKEN_HERE

# Path to Google Drive credentials JSON file (optional, defaults to credentials.json)
GOOGLE_CREDENTIALS_PATH=credentials.json

# Google Drive folder name (optional, defaults to MinecraftMods)
folder_name=Mods

# Google Drive folder ID (optional, if you want to use specific existing folder)
# To get folder ID: open folder in Google Drive, copy ID from URL
folder_id=1ABC-xyz123_folder_id_here

# Upload password (optional, defaults to "gaycraft123")
upload_password=your_secure_password_here
